#!/usr/bin/env python3

import sys
import subprocess
import re

def print_factorization(result):
    factors = re.findall(r'\d+', result)
    
    if len(factors) >= 3:
        num = factors[0]
        factor1 = factors[1]
        factor2 = factors[2]

        if factor2 > factor1:
            factor1, factor2 = factor2, factor1

        print(f"{num}={factor1}*{factor2}")

def main():
    if len(sys.argv) != 2:
        print('Usage: factors <file>')
        sys.exit(1)

    input_file = sys.argv[1]

    with open(input_file, 'r') as file:
        for line in file:
            num = int(line.strip())
            result = subprocess.run(["factor", str(num)], capture_output=True, text=True)
            print_factorization(result.stdout)

if __name__ == "__main__":
    main()
